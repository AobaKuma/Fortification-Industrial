				<!--<dismantlExplosionDam>Smoke</dismantlExplosionDam>-->
				<!-- 分裂爆炸的DamageDef -->
				<!--<dismantlExplosionRadius>0.5</dismantlExplosionRadius>-->
				<!-- 分裂爆炸的範圍 -->
				<!--<dismantlExplosionDamAmount>0</dismantlExplosionDamAmount>-->
				<!-- 分裂爆炸的傷害 -->
				<!--<dismantlExplosionArmorPenetration>0</dismantlExplosionArmorPenetration>-->
				<!-- 分裂爆炸傷害的穿甲 -->
				<!--<dismantlExplosionSound>ExtinguisherExplosion</dismantlExplosionSound>-->
